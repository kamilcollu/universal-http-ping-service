# Environment Variables Template
# Copy this file to .env and update with your actual values

# Website URLs to keep alive (comma-separated)
WEBSITE_URLS=https://www.example1.com,https://www.example2.com,https://www.example3.com

# Ping interval in cron format (default: every 14 minutes)
PING_INTERVAL=*/14 * * * *

# Delay between individual pings in milliseconds (default: 1000ms)
PING_DELAY=1000

# Timeout for HTTP requests in milliseconds (default: 30000ms)
REQUEST_TIMEOUT=30000

# Log level: 'info', 'warn', 'error' (default: 'info')
LOG_LEVEL=info

# Timezone for logging (default: 'UTC')
TIMEZONE=UTC
